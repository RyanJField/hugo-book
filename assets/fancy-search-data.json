{
  {{- $pages := where .Site.Pages "Kind" "in" (slice "home" "page" "section") }}
    {{- $first:= true}}
    {{- range $index, $page := $pages }}
      {{- $i :=add $index 1 }}
      {{- if $page.Content }}
      {{- if not $first }}
      ,
      {{- end -}}
      
        "{{$i}}": {
        "url": "{{ $page.RelPermalink }}",
        "relUrl": "{{ $page.RelPermalink }}",
        "title": {{ (partial "docs/title" $page) | jsonify }},
        "doc": {{ (partial "docs/title" $page) | jsonify }},
        "section": {{- if or (eq $page.Parent 0) (eq $page.Parent "") }} "" {{ else }} {{ (partial "docs/title" $page.Parent) | jsonify }} {{- end -}},
        "content": {{$page.Plain | jsonify}}
      }
            
      {{- $first = false}}
      {{- end -}}
    {{- end -}}
  }
  